Checks: misc-forbid-loops,misc-forbid-arrays,misc-forbid-functions,misc-forbid-stl,readability-identifier-naming,misc-include-cleaner
WarningsAsErrors: misc-forbid-*

CheckOptions:

- key: misc-forbid-functions.ForbiddenNames
	value: sort,printf,malloc

- key: readability-identifier-naming.FunctionCase
	value: camelBack

- key: readability-identifier-naming.VariableCase
	value: snake_case
